msgstr ""
"  -D,  --domains=LISTE        izin verilecek alan isimlerinin virgül "
"ayraçlı\n"
"                              listesi\n"

#: src/main.c:757
msgid ""
"       --exclude-domains=LIST      comma-separated list of rejected "
"domains.\n"
msgstr ""
"       --exclude-domains=LISTE\n"
